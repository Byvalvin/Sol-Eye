{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python",
        "config": {
          "pythonVersion": "3.8"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ],
    "installCommand": "pip install setuptools && pip install --upgrade setuptools && pip install -r requirements.txt"
  }
  